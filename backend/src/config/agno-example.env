# Agno Monitoring Configuration
# Copy relevant settings to .env

# Agno API Configuration
AGNO_API_KEY=your-agno-api-key-here
AGNO_PROJECT_ID=your-project-id-here
AGNO_ENDPOINT=https://api.agno.com

# Monitoring Settings
AGNO_BATCH_SIZE=100
AGNO_FLUSH_INTERVAL=10000
AGNO_ENVIRONMENT=development

# Feature Flags
AGNO_ENABLE_METRICS=true
AGNO_ENABLE_EVENTS=true
AGNO_ENABLE_TRACES=true
AGNO_ENABLE_ERROR_TRACKING=true

# Performance Settings
AGNO_MAX_BUFFER_SIZE=1000
AGNO_RETRY_ATTEMPTS=3
AGNO_TIMEOUT=5000