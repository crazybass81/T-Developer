{
  "name": "t-developer-backend",
  "version": "1.0.0",
  "description": "T-Developer Backend Services",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=utils",
    "test:e2e": "jest --config jest.e2e.config.js",
    "start:test": "NODE_ENV=test ts-node src/server.ts",
    "test:seed": "ts-node tests/fixtures/seed-runner.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:report": "npm run test:coverage && echo 'ðŸ“Š Test report generated at test-reports/index.html'",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "generate-key": "ts-node ../scripts/generate-crypto-key.ts",
    "encrypt-env": "ts-node ../scripts/encrypt-env.ts",
    "setup-secrets": "ts-node ../scripts/setup-secrets.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "express": "^4.18.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "bcrypt": "^5.1.0",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "node-statsd": "^0.1.1",
    "winston-cloudwatch": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "aws-sdk-client-mock": "^3.0.0",
    "@faker-js/faker": "^8.3.1",
    "jest-html-reporter": "^3.10.2",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0"
  }
}