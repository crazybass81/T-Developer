{
  "name": "TaskFilterAgent",
  "type": "query_processor",
  "config": {
    "filters": {
      "status": [
        "all",
        "active",
        "completed"
      ],
      "priority": [
        "high",
        "medium",
        "low"
      ],
      "category": "dynamic",
      "dateRange": "custom"
    },
    "search": {
      "fields": [
        "title",
        "description",
        "tags"
      ],
      "fuzzy": true,
      "highlighting": true
    },
    "sort_options": [
      "priority",
      "dueDate",
      "createdAt",
      "title"
    ]
  }
}