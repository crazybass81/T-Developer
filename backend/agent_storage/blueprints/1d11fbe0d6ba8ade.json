{
  "name": "TodoCRUDAgent",
  "type": "data_manager",
  "config": {
    "entity": "Task",
    "operations": [
      "create",
      "read",
      "update",
      "delete",
      "list"
    ],
    "fields": {
      "id": "string",
      "title": "string",
      "description": "string",
      "completed": "boolean",
      "priority": "number",
      "dueDate": "date",
      "category": "string",
      "tags": "array"
    },
    "validation_rules": {
      "title": {
        "required": true,
        "minLength": 1,
        "maxLength": 200
      },
      "priority": {
        "min": 1,
        "max": 5
      }
    }
  }
}