{
  "name": "t-developer-mvp",
  "version": "1.0.0",
  "description": "AI-powered multi-agent development platform",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "npm run preset:activate testing && npm run test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "seed:dev": "ts-node scripts/seed-dev-data.ts",
    "build:frontend": "echo 'Frontend build placeholder'",
    "preset:list": "ts-node scripts/preset-manager.ts list",
    "preset:activate": "ts-node scripts/preset-manager.ts activate",
    "preset:deactivate": "ts-node scripts/preset-manager.ts deactivate",
    "dev:minimal": "npm run preset:activate minimal && npm run dev",
    "dev:mocks": "npm run preset:activate full-mocks && npm run dev",
    "db:setup:test": "ts-node scripts/setup-test-db.ts",
    "db:cleanup:test": "ts-node scripts/cleanup-test-db.ts",
    "test:agents": "ts-node scripts/test-agent-framework.ts",
    "agents:list": "echo 'Available agent types:'",
    "test:communication": "ts-node scripts/test-communication.ts",
    "test:bedrock": "ts-node scripts/test-bedrock-integration.ts",
    "test:squad": "ts-node scripts/test-agent-squad.ts",
    "test:agno": "ts-node scripts/test-agno-monitoring.ts",
    "generate": "ts-node scripts/code-generator/generator.ts",
    "generate:agent": "npm run generate agent",
    "generate:api": "npm run generate api",
    "verify:env": "ts-node scripts/verify-environment.ts",
    "phase1:init": "ts-node scripts/init-phase1.ts",
    "phase0:archive": "./scripts/archive-phase0.sh",
    "crypto:generate-key": "ts-node scripts/generate-crypto-key.ts",
    "crypto:encrypt-env": "ts-node scripts/encrypt-env.ts",
    "security:test": "jest --testPathPattern=security",
    "db:local:setup": "ts-node scripts/setup-local-db.ts",
    "aws:setup-profile": "python scripts/setup-aws-profile.py",
    "aws:request-bedrock": "ts-node scripts/request-bedrock-access.ts",
    "monitoring:start": "docker-compose -f docker-compose.monitoring.yml up -d",
    "monitoring:stop": "docker-compose -f docker-compose.monitoring.yml down",
    "phase0:validate": "ts-node scripts/validate-phase0-completion.ts",
    "phase0:check": "ts-node scripts/phase0-completion-check.ts",
    "cleanup": "ts-node scripts/cleanup-and-restructure.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.0.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@faker-js/faker": "^8.4.1",
    "@types/ioredis": "^4.28.10",
    "aws-sdk-client-mock": "^3.1.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-statsd": "^0.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "winston-cloudwatch": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.0",
    "@types/helmet": "^4.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "ws": "^8.14.2"
  },
  "keywords": [
    "ai",
    "multi-agent",
    "development",
    "aws"
  ],
  "author": "T-Developer Team",
  "license": "MIT"
}
