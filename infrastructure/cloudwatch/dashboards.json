{
  "DashboardBody": {
    "widgets": [
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/AgentCore", "EndpointLatency", {"stat": "Average"}],
            [".", ".", {"stat": "p99"}],
            [".", "EndpointErrors", {"stat": "Sum"}],
            [".", "EndpointRequests", {"stat": "Sum"}]
          ],
          "period": 300,
          "stat": "Average",
          "region": "us-east-1",
          "title": "AgentCore API Performance",
          "yAxis": {
            "left": {
              "min": 0
            }
          },
          "view": "timeSeries",
          "stacked": false
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/Squad", "WorkflowExecutions", {"stat": "Sum"}],
            [".", "WorkflowSuccessRate", {"stat": "Average"}],
            [".", "ParallelTasks", {"stat": "Maximum"}],
            [".", "QueueDepth", {"stat": "Average"}]
          ],
          "period": 300,
          "stat": "Average",
          "region": "us-east-1",
          "title": "Agent Squad Orchestration",
          "view": "timeSeries"
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/Evolution", "AgentMemoryUsage", {"stat": "Average"}],
            [".", ".", {"stat": "Maximum"}],
            [".", "InstantiationTime", {"stat": "Average"}],
            [".", ".", {"stat": "p99"}]
          ],
          "period": 300,
          "region": "us-east-1",
          "title": "Performance Constraints (6.5KB/3μs)",
          "yAxis": {
            "left": {
              "min": 0,
              "max": 6500
            },
            "right": {
              "min": 0,
              "max": 3
            }
          },
          "annotations": {
            "horizontal": [
              {
                "label": "Memory Limit (6.5KB)",
                "value": 6500,
                "yAxis": "left"
              },
              {
                "label": "Instantiation Limit (3μs)",
                "value": 3,
                "yAxis": "right"
              }
            ]
          }
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/Anomaly", "AnomaliesDetected", {"stat": "Sum"}],
            [".", "AnomalyScore", {"stat": "Average"}],
            [".", "FalsePositives", {"stat": "Sum"}],
            [".", "DetectionLatency", {"stat": "Average"}]
          ],
          "period": 300,
          "region": "us-east-1",
          "title": "Anomaly Detection System",
          "view": "timeSeries"
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["AWS/Bedrock", "InvocationCount", {"stat": "Sum"}],
            [".", "InvocationLatency", {"stat": "Average"}],
            [".", "InvocationErrors", {"stat": "Sum"}],
            [".", "TokensProcessed", {"stat": "Sum"}]
          ],
          "period": 300,
          "region": "us-east-1",
          "title": "Bedrock Agent Performance",
          "view": "timeSeries"
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/API", "RequestCount", {"stat": "Sum"}],
            [".", "ErrorRate", {"stat": "Average"}],
            [".", "Availability", {"stat": "Average"}],
            [".", "Latency", {"stat": "Average", "label": "Avg Latency"}],
            [".", ".", {"stat": "p99", "label": "P99 Latency"}]
          ],
          "period": 300,
          "region": "us-east-1",
          "title": "API Gateway Metrics",
          "view": "singleValue"
        }
      },
      {
        "type": "log",
        "properties": {
          "query": "SOURCE '/aws/lambda/t-developer-agent-core'\n| fields @timestamp, @message\n| filter @message like /ERROR/\n| sort @timestamp desc\n| limit 20",
          "region": "us-east-1",
          "title": "Recent Errors",
          "queryLanguage": "kusto"
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/Cost", "AIApiCost", {"stat": "Sum"}],
            [".", "AWSResourceCost", {"stat": "Sum"}],
            [".", "TotalCost", {"stat": "Sum"}],
            [".", "CostSavings", {"stat": "Average"}]
          ],
          "period": 86400,
          "region": "us-east-1",
          "title": "Cost Optimization (Target: 30% Reduction)",
          "view": "number",
          "annotations": {
            "horizontal": [
              {
                "label": "Cost Target",
                "value": 0.7
              }
            ]
          }
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/Health", "EndpointHealth", {"stat": "Average"}],
            [".", "SystemHealth", {"stat": "Average"}],
            [".", "DatabaseHealth", {"stat": "Average"}],
            [".", "CacheHealth", {"stat": "Average"}]
          ],
          "period": 60,
          "region": "us-east-1",
          "title": "System Health Status",
          "view": "singleValue",
          "setPeriodToTimeRange": false
        }
      },
      {
        "type": "metric",
        "properties": {
          "metrics": [
            ["T-Developer/SLA", "Uptime", {"stat": "Average"}],
            [".", "ResponseTime", {"stat": "Average"}],
            [".", "ErrorRate", {"stat": "Average"}],
            [".", "SLACompliance", {"stat": "Average"}]
          ],
          "period": 3600,
          "region": "us-east-1",
          "title": "SLA Compliance (Target: 99.9%)",
          "view": "gauge",
          "yAxis": {
            "left": {
              "min": 95,
              "max": 100
            }
          }
        }
      }
    ]
  },
  "DashboardName": "T-Developer-Production-Dashboard",
  "PeriodOverride": "inherit"
}
