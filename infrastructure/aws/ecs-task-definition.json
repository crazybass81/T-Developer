{
  "family": "t-developer-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::036284794745:role/t-developer-mvp-ecs-execution-role-development",
  "taskRoleArn": "arn:aws:iam::036284794745:role/t-developer-mvp-ecs-task-role-development",
  "containerDefinitions": [
    {
      "name": "t-developer-backend",
      "image": "036284794745.dkr.ecr.us-east-1.amazonaws.com/t-developer-backend:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "T_DEVELOPER_PROJECTS_BUCKET",
          "value": "t-developer-mvp-projects-development-036284794745"
        },
        {
          "name": "T_DEVELOPER_ASSETS_BUCKET",
          "value": "t-developer-mvp-assets-development-036284794745"
        },
        {
          "name": "T_DEVELOPER_PROJECTS_TABLE",
          "value": "t-developer-mvp-projects-development"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/t-developer-mvp-development",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}