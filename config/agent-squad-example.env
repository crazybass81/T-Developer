# AWS Agent Squad Configuration
# Copy relevant settings to .env

# Squad Configuration
SQUAD_SUPERVISOR_NAME=T-Developer-Squad
SQUAD_SUPERVISOR_ROLE=orchestrator

# Communication
REDIS_URL=redis://localhost:6379
SQUAD_COMMUNICATION_PROTOCOL=redis

# Worker Configuration
SQUAD_NL_WORKERS=2
SQUAD_UI_WORKERS=1
SQUAD_SEARCH_WORKERS=3

# Performance Settings
SQUAD_TASK_TIMEOUT=30000
SQUAD_MAX_QUEUE_SIZE=1000
SQUAD_WORKER_RETRY_ATTEMPTS=3

# Monitoring
SQUAD_ENABLE_METRICS=true
SQUAD_METRICS_INTERVAL=5000